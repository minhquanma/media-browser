{
  "compilerOptions": {
    "checkJs": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "api/*": ["api/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "services/*": ["services/*"],
      "utils/*": ["utils/*"]
    }
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
